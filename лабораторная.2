import networkx as nx

# Создаем граф
G = nx.Graph()
G.add_edges_from([(1, 2), (1, 3), (1, 4), (2, 3), (3, 4)])

# Вычисляем центральность в собственных векторах
eigenvector_centrality = nx.eigenvector_centrality(G)

# Выводим центральность для каждого узла
for node in eigenvector_centrality:
    print(f'Узел {node}: Центральность - {eigenvector_centrality[node]}')

# Находим узлы с одинаковой центральностью
nodes_with_same_centrality = [node for node in eigenvector_centrality if eigenvector_centrality[node] == eigenvector_centrality[1]]

# Находим узел с отличающейся центральностью
node_with_different_centrality = max(eigenvector_centrality, key=eigenvector_centrality.get)

print(f'Узлы с одинаковой центральностью: {nodes_with_same_centrality}')
print(f'Узел с отличающейся центральностью: {node_with_different_centrality}')
